  "id" : "cikm13",
  "title" : "Automated Probabilistic Modeling for Relational Data",
  "pubType" : {
    "enumClass" : "com.github.sameersingh.bibere.PubType",
    "value" : "Conference"
  },
  "year" : 2013,
